    "scale_fieldName":"posi_item_30"
  },
  {
    "itemID":"79",
    "sort_order":"35",
    "scale_name":"EBI",
    "scale_construct":"2.a.ii",
    "scale_direction":"FWD",
    "item_text":"I tell people I'm not hungry even when I really am.",
