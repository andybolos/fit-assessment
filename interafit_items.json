    "scale_fieldName":"basi_item_07"
  },
  {
    "itemID":"76",
    "sort_order":"32",
    "scale_name":"EBI",
    "scale_construct":"1.d.ii",
    "scale_direction":"FWD",
    "item_text":"I don't need to drink much water because of the other things I drink.",
