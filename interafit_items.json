  {
    "itemID":"83",
    "sort_order":"110",
    "scale_name":"RCQ15",
    "scale_construct":"4",
    "scale_direction":"FWD",
    "item_text":"I work hard to make meaningful changes in my life.",
    "scale_tblName":"scale_rcq",
    "scale_fieldName":"rcq_item_12"
  },
  {
    "itemID":"84",
    "sort_order":"107",
    "scale_name":"RCQ15",
    "scale_construct":"1",
    "scale_direction":"FWD",
    "item_text":"My family or friends have been pressuring me to make positive changes in my life.",
